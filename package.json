{
  "name": "CPSWallet",
  "description": "electron",
  "version": "0.0.1",
  "author": "https://www.cpscoin.com",
  "main": "main.js",
  "productName": "CPSWallet",
  "scripts": {
    "start": "npm run build && electron .",
    "dev": "webpack-dev-server --inline --hot --target=web",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --target=electron-renderer",
    "predeploy": "npm run build",
    "deploy": "surge --project . --domain vue-2-simple-routing-example.surge.sh",
    "pack": "electron-packager . CPSWallet --platform=win32  --overwrite --out=./dist/  --no-prune",
    "packall": "yarn build && yarn predeploy &&  electron-packager . CPSWallet --asar=true  --platform=win32,darwin,linux --arch=x64  --overwrite --out=./outapp/ --no-prune  --icon CPSWALLETLOGO",
    "setup": "electron-installer-windows --src outapp/CPSWallet-win32-x64/ --dest outapp/installers/ --icon CPSWALLETLOGO.ico"
  },
  "build": {
    "appId": "com.electron.block",
    "mac": {
      "category": "public.app-category.Business",
      "target": "dmg"
    }
  },
  "dependencies": {
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "bitcore-lib": "^0.15.0",
    "bitcore-mnemonic": "^1.5.0",
    "bluebird": "^3.3.1",
    "browserify": "^13.1.0",
    "buffer": "^4.9.0",
    "chai": "^3.0.0",
    "clipboard": "^2.0.0",
    "cross-env": "^4.0.0",
    "crypto-js": "^3.1.5",
    "css-loader": "^0.28.10",
    "electron": "1.8.4",
    "electron-context-menu": "^0.9.1",
    "electron-default-menu": "^1.0.1",
    "electron-packager": "^11.1.0",
    "electron-prompt": "^0.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "elliptic": "^3.1.0",
    "eth-lightwallet-jh": "3.0.3",
    "ethereumjs-tx": "^1.3.3",
    "ethereumjs-util": "^5.1.1",
    "file-loader": "^0.11.1",
    "hooked-web3-provider": "^1.0.0",
    "istanbul": "^0.3.15",
    "iview": "^2.9.2",
    "iview-loader": "^1.0.0",
    "kjua": "^0.1.2",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "lodash": "^4.17.5",
    "rlp": "^2.0.0",
    "scrypt-async": "^1.2.0",
    "smalltalk": "^2.5.1",
    "style-loader": "^0.20.2",
    "surge": "^0.19.0",
    "tweetnacl": "^1.0.0",
    "uglify-js": "^2.7.2",
    "url-loader": "^1.0.1",
    "vue": "^2.3.3",
    "vue-loader": "^12.0.3",
    "vue-qart": "^2.1.1",
    "vue-template-compiler": "^2.3.3",
    "web3": "0.20.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "devDependencies": {
    "electron-installer-windows": "^1.0.2"
  }
}
